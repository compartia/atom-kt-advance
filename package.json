{
    "name": "atom-kt-advance",
    "main": "./lib/init",
    "version": "0.0.0",
    "description": "KT Advance static C analyser",
    "keywords": [
        "lint",
        "linter",
        "c"
    ],
    "activationCommands": {
        "atom-workspace": "atom-kt-advance:toggle"
    },

    "repository": "https://github.com/compartia/atom-kt-advance",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },

    "dependencies": {
        "atom-package-deps": "^2.0.3"
    },

    "providedServices": {
        "linter": {
            "versions": {
                "1.0.0": "provideLinter"
            }
        }
    },

    "package-deps": [
        "linter"
    ]


}
